function disemvowel(str) {
    let arr = str.split('')
    let arr2 = []
    for (let i = 0; i<arr.length; i++) {
        if(arr[i] !== 'a' && arr[i] !== 'u' && arr[i] !== "o" && arr[i] !== 'i' && arr[i] !== 'e' && arr[i] !== 'A' && arr[i] !== 'U' && arr[i] !== "O" && arr[i] !== 'I' && arr[i] !== 'E') {
            arr2.push(arr[i])
        }
    }
    return arr2.join('')
}
